{% extends "layout.njk" %}

{% block content %}

    <div class="mais-acessadas">
        <div class="mais-acessadas-receitas">
            {% for item in items %}
            <div class="card" id="{{item.id}}">
                <img src="{{item.url}}" alt="{{item.name}}">
                <h3>{{item.name}}</h3>
                <p>{{item.chef}}</p>
            </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}